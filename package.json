{
  "name": "snapweb-toolkit",
  "version": "0.1.1",
  "private": true,
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "main": "lib/bundle.js",
  "module": "src/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "dist": "npm run clean && NODE_ENV=production webpack -p --config ./config/webpack.config.prod.js",
    "build": "npm run clean && NODE_ENV=development webpack -d --config ./config/webpack.config.dev.js",
    "start": "npm run clean && NODE_ENV=development webpack -d --config ./config/webpack.config.dev.js --watch"
  },
  "peerDependencies": {
    "react": "^15.4.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-css-modules": "^2.2.0",
    "babel-preset-react-app": "^2.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "2.0.0-rc.0",
    "file-loader": "^0.9.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.1.6",
    "url-loader": "^0.5.7",
    "vanilla-framework": "1.0.1",
    "webpack": "2.2"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0"
  }
}
