{
  "name": "snapweb-toolkit",
  "version": "0.1.3",
  "private": true,
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "main": "lib/bundle.js",
  "module": "src/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "dist": "npm run clean && NODE_ENV=production webpack -p --config ./config/webpack.config.prod.js",
    "build": "npm run clean && NODE_ENV=development webpack -d --config ./config/webpack.config.dev.js",
    "start": "npm run clean && NODE_ENV=development webpack -d --config ./config/webpack.config.dev.js --watch"
  },
  "peerDependencies": {
    "react": "^15.4.2"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "6.26.3",
    "babel-loader": "6.4.1",
    "babel-plugin-react-css-modules": "2.8.0",
    "babel-preset-react-app": "2.2.0",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "node-sass": "4.12.0",
    "sass-loader": "6.0.7",
    "source-map-loader": "0.2.4",
    "url-loader": "0.6.2",
    "vanilla-framework": "1.8.1",
    "webpack": "4.30.0"
  },
  "dependencies": {
    "react": "15.6.2"
  }
}
